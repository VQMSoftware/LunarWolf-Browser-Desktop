{
  "name": "LunarWolf",
  "version": "0.0.2",
  "license": "GPL-3.0",
  "sideEffects": false,
  "packageManager": "yarn@4.9.2",
  "description": "LunarWolf is a nonprofit, fast free and secure Web Browser. It is based on lunarwolf, and includes news and tons of features out of the box. lunarwolf is resource effecient, and is updated often. Powered by SnailDOS!",
  "keywords": [
    "web-browser",
    "material",
    "electron",
    "react",
    "mobx",
    "styled-components"
  ],
  "homepage": "https://github.com/VQMSoftware/lunarwolf-Browser",
  "main": "build/main.bundle.js",
  "author": "LunarWolf Browser Devs <vqmsoftware@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VQMSoftware/LunarWolf-Browser-Desktop.git"
  },
  "bugs": {
    "url": "https://github.com/VQMSoftware/LunarWolf-Browser-Desktop/issues"
  },
  "scripts": {
    "dev-renderer": "cross-env DEV=1 webpack serve --config webpack.config.renderer.js",
    "dev-webpack": "cross-env DEV=1 webpack",
    "build-renderer": "webpack --config webpack.config.renderer.js",
    "dev": "cross-env START=1 npm run watch",
    "build": "rimraf build && concurrently \"npm run build-renderer\" \"webpack\"",
    "nuke": "rimraf node_modules",
    "ci-build": "node scripts/ci-build.js",
    "start": "electron .",
    "watch": "concurrently \"npm run dev-renderer\" \"npm run dev-webpack\"",
    "compile-windows": "npm run build && electron-builder -w",
    "compile-darwin": "npm run build && electron-builder -m --universal",
    "compile-linux": "npm run build && electron-builder -l",
    "compile-pacman": "npm run build && electron-builder --linux pacman",
    "postinstall": "electron-builder install-app-deps",
    "deploy": "npm run build && electron-builder build --win --linux appimage snap --publish never",
    "lint": "concurrently \"eslint \"src/**/*.ts*\" \"src/**/*.tsx*\"\" \"prettier --check \"src/**/*.ts*\" \"src/**/*.tsx*\"\"",
    "lint-fix": "concurrently \"eslint \"src/**/*.ts*\" \"src/**/*.tsx*\" --fix\" \"prettier --write \"src/**/*.ts*\" \"src/**/*.tsx*\"\"",
    "rebuild": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@cliqz/adblocker-electron": "1.26.14",
    "@electron/remote": "^2.1.3",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
    "@seald-io/nedb": "^4.1.2",
    "@types/animejs": "^3.1.13",
    "@types/chrome": "^0.0.332",
    "@types/crypto-js": "^4.2.2",
    "@types/jszip": "^3.4.1",
    "@types/node": "^24.0.12",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-helmet": "^6.1.11",
    "@types/rimraf": "^4.0.5",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@wexond/rpc-core": "^1.0.3",
    "@wexond/rpc-electron": "^1.0.3",
    "animejs": "3.2.1",
    "awesome-node-loader": "^1.1.1",
    "babel-loader": "^10.0.0",
    "concurrently": "^9.2.0",
    "copy-to-clipboard": "^3.3.3",
    "copy-webpack-plugin": "^13.0.0",
    "cross-env": "^7.0.3",
    "electron": "^37.2.0",
    "electron-builder": "26.0.12",
    "electron-chrome-extensions": "^4.9.0",
    "electron-updater": "6.6.2",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "file-loader": "^6.2.0",
    "file-type": "^21.0.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "html-webpack-plugin": "^5.6.3",
    "icojs": "^0.19.5",
    "inotify": "^1.4.6",
    "jszip": "^3.10.1",
    "mobx": "^6.13.7",
    "mobx-react-lite": "^4.1.0",
    "node-bookmarks-parser": "^2.0.0",
